/* Összes szám
Írj programot, amely a képernyőre írja a 4, 5 és 6 számjegyekből képezhető összes négyjegyű számot!
*/

#include <iostream>
using namespace std;

int main()
{

    for(int i = 4; i <= 6; i++)
    {
        for(int j = 4; j <= 6; j++)
        {
            for(int k = 4; k <= 6; k++)
            {
                for(int m = 4; m <=6; m++)
                {
                    cout << i << j << k << m << endl;
                }
            }
        }
    }

    return 0;
}
