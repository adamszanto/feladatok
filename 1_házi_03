/*

3. Írd ki így 1-től a megadott számig a számokat, minden sorban 3 db szám legyen:
1 1 1
2 2 2
3 3 3
4 4 4
...

*/

#include <iostream>
using namespace std;

int main()
{
    int num;
    cin >> num;

    for(int i = 1; i <= num; i++)
    {
        for(int j = 1; j <=num; j++)
            {
                cout << i << " ";
            }
            cout << "\n";
    }


    return 0;
}
